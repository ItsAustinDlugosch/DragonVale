enum CaveTime: String, Codable {
    case day, night
}
enum CaveWeather: String, Codable {
    case bats, blossoms, butterflies, confetti, dandelions, sunny, fireflies, ghosts, leaves, rain, raimbows, shootingStars, snow, storm, none    
}
enum Cave: String, Codable {
    case normal, social, rift
}
